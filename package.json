{
  "name": "macj",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.8",
    "browser-image-compression": "^2.0.2",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.2.0",
    "html2canvas": "^1.4.1",
    "html2pdf": "^0.0.11",
    "html2pdf.js": "^0.10.1",
    "image-compressor": "^2.0.3",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.8.1",
    "lodash": "^4.17.21",
    "postcss": "^8.4.33",
    "react": "^18.2.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.6",
    "react-easy-crop": "^5.0.5",
    "react-icons": "^4.12.0",
    "react-image-crop": "^11.0.5",
    "react-pdf": "^7.7.1",
    "react-resizable": "^3.0.5",
    "react-router-dom": "^6.21.1",
    "react-scripts": "^5.0.1",
    "uuid": "^9.0.1",
    "wait-on": "^7.2.0",
    "web-vitals": "^2.1.4"
  },
  "main": "public/main.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently -k \"cross-env BROWSER=none npm start\" \"npm run electron:start\"",
    "electron:build": "yarn build && electron-builder -c.extraMetadata.main=build/main.js",
    "electron:start": "wait-on tcp:3000 && electron .",
    "package": "electron-packager . electron-react --platform=win32 --arch=x64 --out=dist --overwrite",
    "package-win": "electron-packager . macj --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"macj\""
  },
  "build": {
    "directories": {
      "output": "dist"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^28.1.1",
    "electron-builder": "^24.9.1",
    "electron-packager": "^17.1.2",
    "tailwindcss": "^3.4.1"
  }
}
